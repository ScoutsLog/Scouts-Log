{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"description": "__MSG_extDescription__",
	"version": "0.3.0",
	"minimum_chrome_version": "43",
	"default_locale": "en",
        "author": "Extension developed by Gary Heil; Korean language translation done by Kyung Lim; some icons used from http://www.famfamfam.com/lab/icons/silk/",
	
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	
	"permissions": [
		"storage",
		"http://eyewire.org/",
		"http://scoutslog.org/"
	],
  
	"browser_action": {
		"default_icon": "images/icon16.png"
	},
  
	"content_scripts": [
		{
			"js": [ "js/version-compare.js", "js/platform.js", "js/ew-plugin.js" ],
			"css": [ "css/scouts.css" ],
			"matches": [ "http://eyewire.org/*" ]
		},
		{
			"js": [ "js/sl-plugin.js" ],
			"matches": [ "http://scoutslog.org/*" ]
		}
	],
  
	"web_accessible_resources": [
		"css/*",
		"js/*",
		"_locales/*",
		"content/*",
		"images/*",
		"http://scoutslog.org/*"
	]
	
}
