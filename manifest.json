{
	"manifest_version": 2,
	"name": "EyeWire Scouts' Log",
	"description": "This extension allows scouts, scythes, and admins to track cell/cube issues within EyeWire.",
	"version": "0.1.6",
	"minimum_chrome_version": "43",
	
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	
	"permissions": [
		"storage",
		"http://eyewire.org/",
		"http://scoutslog.org/"
	],
  
	"browser_action": {
		"default_icon": "images/icon16.png"
	},
  
	"content_scripts": [
		{
			"js": [ "plugin.js" ],
			"css": [ "scouts.css" ],
			"matches": [ "http://eyewire.org/*" ]
		},
		{
			"js": [ "sl-plugin.js" ],
			"matches": [ "http://scoutslog.org/*" ]
		}
	],
  
	"web_accessible_resources": [
		"scouts.js",
		"scouts.css",
		"sl-scouts.js",
		"images/*",
		"http://scoutslog.org/*"
	]
	
}
